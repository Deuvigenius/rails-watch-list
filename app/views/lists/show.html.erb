<div class="container">
  <h1><%= @list.name %></h1>
  <%= link_to "Back to lists", lists_path %>
  <%= link_to "Add Movie", new_list_bookmark_path(@list) %>

    <ul class="list-group">
      <% @list.movies.each do |movie| %>
        <li class="list-group-item"><%= movie.title %></li>
      <% end %>
    </ul>
</div>