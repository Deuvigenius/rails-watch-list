<%= simple_form_for [@list, @bookmark] do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :comment, label: "Add a comment" %>
    <%= f.association :movie, as: :select, collection: Movie.all, label: "Movie" %>
    
  </div>

  <div class="form-actions btn btn-primary">
    <%= f.button :submit %>
  </div>
<% end %>